package main1;

import java.util.Date;

import cls.Board;

public class Main5 {

	public static void main(String[] args) {

		Board board = new Board();
		Date date = new Date();

		board.setBrdNo("1");
		board.setBrdTitle("첫 게시글");
		board.setBrdContent("안녕하세요");
		board.setBrdWriter("junkue20");
		board.setBrdHit(44091);

		board.setBrdDate(date);

		System.out.println(board.toString());
		
	}

}

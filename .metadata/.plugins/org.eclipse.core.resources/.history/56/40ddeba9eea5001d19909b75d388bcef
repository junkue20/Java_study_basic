package day6;

import java.util.Date;


public class ItemMain {

	public static void main(String[] args) {
	ItemDB iDB = new ItemDB();
		
	//dp연결하고 컬렉션을 items로
	iDB.printItems();
	System.out.println();
	
	//물품등록
	Item item = new Item(10000,"abc", "400", 10000, 10, new Date());
	int ret = iDB.insertItem(item); //성공하면 1, 실패시 -1
	System.out.println(ret);		
	
	//전체물품 출력
	iDB.printItems();
	
	
		
    // 물품코드를 이용한 수정	
    // 물품명, 물품내용, 가격, 수량 변경가능
	item = new Item(10000,"abc", "400", 10000, 10, new Date());
	
	
//	int ret = iDB.updateItem(item); //성공하면 1, 실패시 -1
//	System.out.println(ret);
	
	//__________________________________
	}

}

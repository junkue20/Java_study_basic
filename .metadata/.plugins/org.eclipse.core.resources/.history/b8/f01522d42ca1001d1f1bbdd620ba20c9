package main1;

import java.util.Date;

import cls.Item;

public class Main6 {

	public static void main(String[] args) {
		 // 1. 물품등록
		Item obj = new Item();
		obj.setNo(100L);
		obj.setDate(new Date()); // ctrl + shift + O
		obj.setName("사과");
		obj.setContent("달콤함");
		obj.setPrice(9870);
		obj.setQuantity(10000L);

		System.out.println(obj.toString()); // 현재 저장된 내용 확인
	}

}

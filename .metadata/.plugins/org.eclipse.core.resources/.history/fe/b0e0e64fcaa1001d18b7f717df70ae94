package main2;

import java.util.Date;

import cls.Book;

public class Main2 {

	public static void main(String[] args) {

		// 클래스명 객체명 = new 생성자(준비물);
		Book book = new Book (1, "java", "신용권");
		
	
		book.setCate('B');
		book.setDate(new Date());
		book.setPrice(0);
		
		System.out.println(book.toString());
	}

}

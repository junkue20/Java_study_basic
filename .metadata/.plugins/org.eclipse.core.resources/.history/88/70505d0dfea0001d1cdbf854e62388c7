package main1;

public class Main1 {

	public static void main(String[] args) {
		// 정수형 : byte, short, int, long
		// 실수형 : float, double
		// 문자형 : char
		// 논리형 : boolean
		/* 8개의 타입이 존재한다. */

		// 클래스형
		// 정수형 : byte, Short, Integer, Long => 클래스로 만들어 놓은것
		int age = 13;

		//if(조건) age==50, age != 50, age =>50, age<=50
		//if(조건 && 조건) &&and || or ! not
		if( age>=50) {
			System.out.println("50보다 큽니다.");
		}
		else {
			System.out.println("50보다 작습니다.");

		}
		
	}

}

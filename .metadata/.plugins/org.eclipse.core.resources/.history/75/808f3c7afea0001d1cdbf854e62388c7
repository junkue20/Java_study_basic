package main1;

public class Main2 {

	// 변수선언
	public static void main(String[] args) {

		int a=1;
		int b=2;
		int c=3;
		int d=4;
		int e=5;
		
		//배열의 번호는 0부터 시작하여 n번째의 값을 출력하기 위해서는 n-1의 값을 출력시키면 된다.
		int[] age = {1,2,3,4,5,6};  // 나이 변수가 5개 생성.
		System.out.println(age[0]); // 1 출력
		System.out.println(age[4]); // 5 출력
		
	}

}

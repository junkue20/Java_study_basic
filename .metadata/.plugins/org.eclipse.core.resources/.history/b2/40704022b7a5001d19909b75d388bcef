package day6;

public class Main2 {

	public static void main(String[] args) {
		Exam2 ex1 = new Exam2();

		ex1.plusAge(); // age == 1
		ex1.plusAge(); // age == 2
		// ex1.printAge();

		// 새로 만들면 새로 시작함.
		Exam2 ex2 = new Exam2();
		ex2.plusAge(); // age == 1
		// ex2.printAge(); // 고로 3이아니라 1이 출력됨.

		// static 함수의 경우는 따로 객체를 생성하지 않고 메소드를 바로 호출함.
		Exam2.plusAge1();
		Exam2.plusAge1();


	}

}
